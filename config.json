{
  "siteName": "enjoy",
  "themeName": "enjoy",
  "appBase": "app",
  "srcBase": "src",
  "testServerPath": "",
  "url": {
    "local": "http://enjoy.loc",
    "staging": "https://enjoy.globegs.com",
    "production": "https://enjoy.ua"
  },
  "dbPrefix": "bb_",
  "dbCollate": "utf8mb4_unicode_520_ci",
  "dbCharSet": "utf8mb4",
  "jsEntries": [
    "index",
    "page-home",
    "page-catalog",
    "page-contacts",
    "page-product",
    "page-legal",
    "page-default",
    "page-404"
  ],
  "copyAssets": "atlas|avi|mp4|ogv|ogg|webm",
  "images": {
    "inputTypes": "png|jpg|jpeg|heif|tiff|webp|gif",
    "outputTypes": "webp",
    "skipConvert": "gif|webp",
    "sizes": [],
    "development": {
      "jpeg": {
        "progressive": true,
        "force": false
      },
      "png": {
        "compressionLevel": 0,
        "effort": 1,
        "force": false
      },
      "webp": {
        "effort": 0
      },
      "avif": {
        "effort": 0
      }
    },
    "production": {
      "jpeg": {
        "quality": 100
      },
      "png": {
        "quality": 100
      },
      "webp": {
        "quality": 100,
        "effort": 6,
        "lossless": true
      },
      "avif": {
        "quality": 80,
        "lossless": false,
        "effort": 6
      }
    }
  }
}